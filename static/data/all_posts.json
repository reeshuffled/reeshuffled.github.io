---
layout: none
---

[
    {% for post in site.posts %}
        {
            "title"    : {{ post.title | jsonify | strip_html }},
            "tags"     : "{{ post.tags | join: ', ' }}",
            "url"      : "{{ post.url }}",
            "date"     : "{{ post.date | date: "%b %-d, %Y" }}",
            "content"  : {{ post.content | jsonify | strip_html }},
            "type": "{{post.type}}"
        }{% unless forloop.last %},{% endunless %}
    {% endfor %}
]